const number = 5;

console.log(`Таблиця множення для числа ${number}:`);
for (let i = 1; i <= 10; i++) {
    console.log(`${number} * ${i} = ${number * i}`);
}

console.log(`Таблиця множення для числа ${number}:`);
let i = 1;
while (i <= 10) {
    console.log(`${number} * ${i} = ${number * i}`);
    i++;
}
